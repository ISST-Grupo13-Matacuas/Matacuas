<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Isst_grupo13_matacuas</servlet-name>
		<servlet-class>isst.grupo13.matacuas.Isst_grupo13_matacuasServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Isst_grupo13_matacuas</servlet-name>
		<url-pattern>/matacuas</url-pattern>
	</servlet-mapping>
	<servlet>
	   
		<servlet-name>PruebaBDServlet</servlet-name>
		<servlet-class>isst.grupo13.matacuas.pruebaBD.PruebaBDServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <load-on-startup>1</load-on-startup>
		<servlet-name>PruebaBDServlet</servlet-name>
		<url-pattern>/prueba</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>NuevaQuejaServlet</servlet-name>
		<servlet-class>isst.grupo13.matacuas.NuevaQuejaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NuevaQuejaServlet</servlet-name>
		<url-pattern>/newQueja</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VerQuejaServlet</servlet-name>
		<servlet-class>isst.grupo13.matacuas.VerQuejaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VerQuejaServlet</servlet-name>
		<url-pattern>/verQueja</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PerfilServlet</servlet-name>
		<servlet-class>isst.grupo13.matacuas.PerfilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PerfilServlet</servlet-name>
		<url-pattern>/perfil</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
